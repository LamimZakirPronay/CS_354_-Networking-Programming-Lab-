    "import socket\n",
    "\n",
    "\n",
    "s = socket.socket(socket.AF_INET,socket.SOCK_STREAM)\n",
    "s.connect(('localhost',1234))\n",
    "add=input()\n",
    "\n",
    "s.send(add.encode())\n",
    "mac=s.recv(1024)\n",
    "mac=mac.decode(\"utf-8\")\n",
    "print(mac)\n"
